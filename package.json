{
    "name": "react-starter-template",
    "version": "1.0.0",
    "description": "A starter template for react-based projects",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "start": "rm -rf dist .parcel-cache && parcel src/index.html --open",
        "build": "rm -rf dist .parcel-cache && parcel build src/index.html --public-url ./ --no-source-maps"
