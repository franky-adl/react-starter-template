        "eslint-plugin-react-hooks": "^4.6.2",
        "parcel": "^2.12.0",
        "process": "^0.11.10"
    },
    "dependencies": {
        "gsap": "^3.12.5",
        "react": "^18.3.1",
        "react-cookie": "^7.1.4",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.24.1",
        "styled-components": "^6.1.11"
    }
}
